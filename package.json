{
  "name": "2maps",
  "version": "2.0.0",
  "description": "Maps app based on mapbox",
  "main": "src/index.js",
  "scripts": {
    "start": "node --unhandled-rejections=strict ./dist/app.js",
    "build": "tsc",
    "dev": "node --env-file=.env --inspect -r ts-node/register ./src/app.ts",
    "debug": "export DEBUG=* && npm run start",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:generate  -d ./src/ormconfig.ts src/m/generated",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:run  -d ./src/ormconfig.ts",
    "migration:show": "ts-node node_modules/.bin/typeorm migration:show",
    "test": "NODE_ENV=test mocha --no-parallel --require ts-node/register src/*.spec.ts src/**/*.spec.ts",
    "t": "NODE_ENV=test mocha --no-parallel --require ts-node/register src/routes/kmlUtils.spec.ts",
    "r": "tsc && node --unhandled-rejections=strict ./dist/tools/mapLoader.js",
    "c": "tsc && node --unhandled-rejections=strict ./dist/tools/joinSqlDbs.js",
    "wiki": "tsc && node --unhandled-rejections=strict ./dist/tools/wikimapia.js",
    "lint": "tslint 'src/**/*.ts' --fix; eslint 'ui/**/*.js' --fix",
    "m": "ts-node ./pg2sqlite.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikhail-angelov/2maps.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikhail-angelov/2maps/issues"
  },
  "homepage": "https://github.com/mikhail-angelov/2maps#readme",
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@mapbox/togeojson": "^0.16.0",
    "aws-sdk": "^2.1040.0",
    "axios": "^1.6.5",
    "bcrypt-nodejs": "0.0.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "debug": "^4.3.2",
    "ejs": "^3.1.9",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "fast-xml-parser": "^4.1.3",
    "form-data": "^4.0.0",
    "geojson": "^0.5.0",
    "geojson-vt": "^3.2.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mailgun.js": "^3.5.1",
    "multer": "^1.4.2",
    "pg": "^8.7.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "vt-pbf": "^3.1.3",
    "winston": "^3.3.3",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/chai": "^4.2.21",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/mocha": "^8.2.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.2",
    "@types/xmldom": "^0.1.31",
    "chai": "^4.3.4",
    "docker-compose": "^0.24.7",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "less": "^3.13.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.6",
    "sharp": "^0.33.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "workbox-cli": "^6.1.1"
  }
}
