{
  "name": "2maps",
  "version": "1.0.1",
  "description": "map tiles for old NN maps",
  "main": "src/index.js",
  "scripts": {
    "start": "node --unhandled-rejections=strict ./dist/app.js",
    "dev": "export $(cat .env.local | xargs) && ts-node ./src/app.ts",
    "build": "tsc",
    "debug": "export DEBUG=* && npm run start",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert",
    "migration:show": "ts-node node_modules/.bin/typeorm migration:show",
    "test": "NODE_ENV=test mocha --no-parallel --require ts-node/register src/*.spec.ts src/**/*.spec.ts",
    "t": "NODE_ENV=test mocha --no-parallel --require ts-node/register src/routes/mapLoader.spec.ts",
    "r": "tsc && node --unhandled-rejections=strict ./dist/tools/mapLoader.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikhail-angelov/2maps.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikhail-angelov/2maps/issues"
  },
  "homepage": "https://github.com/mikhail-angelov/2maps#readme",
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@types/node": "^17.0.2",
    "aws-sdk": "^2.1040.0",
    "axios": "^0.21.4",
    "bcrypt-nodejs": "0.0.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "form-data": "^4.0.0",
    "geojson": "^0.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mailgun.js": "^3.5.1",
    "multer": "^1.4.2",
    "nestjs-db-unit": "^2.3.0",
    "pg": "^8.7.1",
    "sqlite3": "^5.0.0",
    "ts-node": "^10.1.0",
    "typeorm": "^0.2.34",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/chai": "^4.2.21",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.8",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/mocha": "^8.2.3",
    "@types/multer": "^1.4.7",
    "chai": "^4.3.4",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.6",
    "source-map-support": "^0.5.19",
    "tslint": "^6.1.3"
  }
}
